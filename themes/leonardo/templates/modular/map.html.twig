<div class="modular-row testimonials {{page.header.classes}} bg-white">
    <div class="testimonial container w-10/12 mx-auto py-4 lg:py-16">
		<div class="flex flex-row flex-wrap">
			{% set test = page.header.testimonials|first %}
				<div class="lg:w-3/6 content lg:py-12 md:pr-10 w-full">
						{{page.content}}
				</div>
				<div class="lg:w-3/6 w-full">
					<div class="h-full rounded shadow-lg bg-white p-4">
                    <iframe src="https://www.google.com/maps/embed/v1/place?key={{upkeep.places_api_key}}&q=place_id:{{upkeep.place_id}}" 
                        width="100%"
                        height="100%"
                        async defer 
                        class="rounded"
                        style="min-height:300px"
                    ></iframe>
                </div>
				</div>
		</div>
    </div>
</div>